                var json = JSON.parse(xhr.responseText);
                var html = "";
                html += " <div class='row'><div class='form-group col-lg-3'><div class='fs'>Information No.</div><p><strong>" + json.INFO_NO + "</strong></p></div><div class='form-group col-lg-3'> " +
